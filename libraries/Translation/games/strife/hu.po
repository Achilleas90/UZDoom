msgid "TXT_RYES1_SCRIPT34_D9096_THATS"
msgstr "Ez szerintem megteszi."

msgid "TXT_RNO1_SCRIPT34_D9096_LETME"
msgstr "Hadd futtassak pár tesztet előtte."

msgid "TXT_DLG_SCRIPT34_D10612_THATS"
msgstr "Ennyit tudtam kezdeni most az implantátummal. Esetleg valami gyógyítás?"

msgid "TXT_RPLY0_SCRIPT34_D10612_YEAH"
msgstr "Igen."

msgid "TXT_RYES0_SCRIPT34_D10612_BOYYO"
msgstr "Öregem, elég rossz passzban vagy. Megnézem mit tudok tenni."

