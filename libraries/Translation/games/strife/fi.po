msgid "TXT_DLG_SCRIPT17_D4548_NOTHI"
msgstr "Täällä ei ole mitään nähtävää. Jatkakaa matkaa."

msgid "TXT_DLG_SCRIPT17_D6064_WHATA"
msgstr "No siinäpä vasta mallitapaus. Et sattumoisin ole avun tarpeessa?"

msgid "TXT_RPLY0_SCRIPT17_D6064_WELLY"
msgstr "Itse asiassa, kyllä. Macil lähetti minut."

msgid "TXT_DLG_SCRIPT17_D7580_SHHHH"
msgstr ""
"Hyss, suuta soukemmalle, ellet halua tapattaa meitä molempia. No niin, miten "
"voin auttaa?"

msgid "TXT_RPLY0_SCRIPT17_D7580_TELLM"
